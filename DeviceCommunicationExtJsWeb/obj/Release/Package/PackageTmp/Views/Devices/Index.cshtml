@{
    ViewBag.Title = "Device Communication";
}

<script src="@Url.Content("~/app/app.js")" type="text/javascript"></script>

<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false">  </script>


<script type="text/javascript">
    Ext.Loader.setPath('Ext.ux', '@Url.Content("~/Scripts/extjs/ux")');
    Ext.require([
        'Ext.window.*',
        'Ext.ux.grid.FiltersFeature',
        'Ext.ux.GMapPanel'
    ]);

    window.BaseUrl = '@Url.Action("", "Devices", null, null)';
    window.ContentUrl = '@Url.Content("~/Content/")';

    window.TrackingAction = '@Url.Action("Tracking", "Devices")';
    window.MessagesAction = '@Url.Action("Messages", "Devices")';
    window.LogsAction = '@Url.Action("Logs", "Devices")';
    window.BypassAction = '@Url.Action("Bypass", "Devices")';

    window.GetGeopoints = '@Url.Action("GetGeopoints", "Devices", null, null)';

    window.direction_up_icon = '@Url.Content("~/Content/heading/direction_up.png")';
    window.direction_upright_icon = '@Url.Content("~/Content/heading/direction_upright.png")';
    window.direction_right_icon = '@Url.Content("~/Content/heading/direction_right.png")';
    window.direction_upleft_icon = '@Url.Content("~/Content/heading/direction_upleft.png")';
    window.direction_downright_icon = '@Url.Content("~/Content/heading/direction_downright.png")';
    window.direction_left_icon = '@Url.Content("~/Content/heading/direction_left.png")';
    window.direction_down_icon = '@Url.Content("~/Content/heading/direction_down.png")';
    window.direction_downleft_icon = '@Url.Content("~/Content/heading/direction_downleft.png")';

    Ext.onReady(function () {
        //console.log(window.ContentUrl);

        //console.log("BaseUrl" + window.BaseUrl);
    });
</script>

